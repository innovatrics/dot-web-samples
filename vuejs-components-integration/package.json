{
  "name": "vuejs-components-integration",
  "private": true,
  "version": "1.0.24",
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "start": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "copy-wasm": "cp -R ./node_modules/@innovatrics/dot-document-auto-capture/wasm ./public"
  },
  "dependencies": {
    "@innovatrics/dot-auto-capture-ui": "6.1.4",
    "@innovatrics/dot-document-auto-capture": "6.1.4",
    "@innovatrics/dot-face-auto-capture": "6.1.4",
    "@innovatrics/dot-magnifeye-liveness": "6.1.4",
    "@innovatrics/dot-smile-liveness": "6.1.4",
    "vue": "^3.4.34"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vue-tsc": "^2.0.29"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
