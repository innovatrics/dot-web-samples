{
  "name": "react-ui-components-customization",
  "private": true,
  "version": "1.3.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "delete-assets": "rm -rf ./public/dot-assets",
    "copy-assets": "find -L node_modules/@innovatrics -type d -name 'dot-assets' -exec cp -R {} ./public \\;",
    "copy-assets-win": "for /d %i in (node_modules\\.pnpm\\@innovatrics\\*) do xcopy /E /I /Y %i\\dot-assets public\\dot-assets"
  },
  "dependencies": {
    "@innovatrics/dot-face-auto-capture": "8.0.3",
    "clsx": "^2.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
